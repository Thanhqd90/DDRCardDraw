{
  "private": true,
  "name": "ddr-card-draw",
  "version": "1.0.0",
  "description": "A card draw app for running DDR tournaments",
  "scripts": {
    "start": "webpack-dev-server --env.dev=true",
    "build": "npm run validate && webpack",
    "build:zip": "npm run validate && webpack --env.target=zip",
    "deploy": "npm run validate && webpack --env.target=surge && surge ./dist https://ddrdraw.surge.sh",
    "deploy:staging": "npm run validate && webpack --env.target=surge && surge ./dist https://ddrdraw-staging.surge.sh",
    "deploy:firebase": "npm run build --env.target=firebase && firebase deploy",
    "migrate": "node scripts/migrate.js",
    "validate": "npm run validate:json && npm run validate:ts",
    "validate:json": "node scripts/validate.js",
    "validate:ts": "tsc --noemit"
  },
  "author": "Noah Manneschmidt",
  "contributors": [
    "Jeff Lloyd",
    "Chris Chike"
  ],
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.8",
    "@babel/plugin-proposal-class-properties": "^7.8",
    "@babel/plugin-proposal-optional-chaining": "^7.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8",
    "@babel/plugin-transform-react-jsx": "^7.8",
    "@babel/plugin-transform-react-jsx-source": "^7.8",
    "@babel/preset-env": "^7.8",
    "@babel/preset-typescript": "^7.8",
    "@blueprintjs/core": "^3.24.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@types/classnames": "^2.2.9",
    "@types/fuzzy-search": "^2.1.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.0",
    "css-loader": "^3.4.0",
    "del-webpack-plugin": "^1.2.0",
    "eslint": "^6.8.0",
    "file-loader": "^5.0.2",
    "firebase": "^7.6.0",
    "firebase-tools": "^7.11.0",
    "fork-ts-checker-webpack-plugin": "^4.0.4",
    "formdata-polyfill": "^3.0.19",
    "fuzzy-search": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.2.1",
    "json-schema-to-typescript": "^8.0.0",
    "jsonschema": "^1.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "offline-plugin": "^5.0.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-intl": "^4.1.1",
    "react-router-dom": "^5.1.2",
    "style-loader": "^1.1.0",
    "surge": "^0.21.3",
    "svg-inline-loader": "^0.8.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.0",
    "zip-webpack-plugin": "^3.0.0"
  }
}
